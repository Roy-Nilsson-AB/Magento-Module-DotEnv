{
    "name": "rnab/module-dotenv",
    "description": "Magento 2 module that provides .env file support using Symfony DotEnv component",
    "type": "magento2-module",
    "license": "proprietary",
    "authors": [
        {
            "name": "Roy Nilsson"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "symfony/dotenv": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "files": [
            "src/prepend.php"
        ],
        "psr-4": {
            "Rnab\\DotEnv\\": "src/"
        }
    }
}
